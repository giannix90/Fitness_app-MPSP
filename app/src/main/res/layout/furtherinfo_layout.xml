<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.example.gianni.mpsp.MainActivity">

        <TextView
            android:layout_width="18dp"
            android:layout_height="18dp"
            android:background="#FF56B7F1"
            android:layout_marginLeft="150dp"
            android:layout_marginTop="92dp"
            android:translationZ="10dp"
            android:clickable="true"
            />
        <TextView
            android:layout_width="18dp"
            android:layout_height="18dp"
            android:background="#FFCDA67F"
            android:layout_marginLeft="150dp"
            android:layout_marginTop="126dp"
            android:translationZ="10dp"
            />
        <TextView
            android:layout_width="18dp"
            android:layout_height="18dp"
            android:background="#FF63CBB0"
            android:layout_marginLeft="150dp"
            android:layout_marginTop="160dp"
            android:translationZ="10dp"
            />

        <org.eazegraph.lib.charts.StackedBarChart
            xmlns:app="http://schemas.android.com/apk/res-auto"
            android:id="@+id/stackedbarchart"
            android:layout_width="120dp"
            android:layout_height="256dp"
            android:padding="10dp"
            android:layout_marginLeft="0dp"
            android:translationZ="10dp"
            app:egFixedBarWidth="true"
            app:egBarWidth="100dp"
            app:egShowValues="true"
            app:egAnimationTime="1500"
            app:egLegendHeight="40dp"/>
        <com.dexafree.materialList.view.MaterialListView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/furtherinfotab"
            android:layout_marginLeft="130dp"
            android:layout_marginTop="0dp"
            android:translationZ="1dp"
            />

        <com.everseat.textviewlabel.TextViewLabel
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Hello World!"
            android:textSize="16sp"
            android:textColor="#FFF"
            android:layout_marginTop="250dp"
            android:layout_centerHorizontal="true"
            app:labelColor="@color/darkgrey"
            android:id="@+id/totalcalories"/>


</RelativeLayout>